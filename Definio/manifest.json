{
   "action": {
      "default_popup": "popup.html"
   },
   "author": "Zuka Murvanidze",
   "background": {
      "service_worker": "src/serviceWorker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "css": [ "/css/inlinePopup.css" ],
      "js": [ "/src/contentScripts.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_end"
   }, {
      "js": [ "src/authContentScript.js" ],
      "matches": [ "https://definio.org/*", "https://staging.definio.org/*" ]
   } ],
   "description": "AI tool for searching, learning & productivity. Features GPT3 Turbo, multiple dictionaries, flashcard learning, & progress tracking",
   "icons": {
      "128": "public/definio-logo-128.png",
      "16": "/public/definio-logo-16.png",
      "48": "public/definio-logo-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Definio: GPT-3 Turbo AI, Dictionary, Learning",
   "permissions": [ "storage" ],
   "short_name": "Definio",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.8"
}
